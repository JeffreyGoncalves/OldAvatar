                                                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                                            </DoubleAnimationUsingKeyFrames>
                                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="FastButtonDisabled">
                                                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                                            </DoubleAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </vsm:VisualState>
                                    <vsm:VisualState x:Name="MouseOver"/>
                                    <vsm:VisualState x:Name="Pressed">
                                        <Storyboard>
                                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="FastButtonEnabled">
                                                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                                            </DoubleAnimationUsingKeyFrames>
                                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="FastButtonDisabled">
                                                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                                            </DoubleAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </vsm:VisualState>
                                    <vsm:VisualState x:Name="Disabled">
                                        <Storyboard>
                                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="FastButton">
                                                <EasingDoubleKeyFrame KeyTime="0" Value="0.5"/>
                                            </DoubleAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </vsm:VisualState>
                                </vsm:VisualStateGroup>
                                <vsm:VisualStateGroup x:Name="CheckStates">
                                    <vsm:VisualState x:Name="Checked"/>
                                    <vsm:VisualState x:Name="Unchecked"/>
                                    <vsm:VisualState x:Name="Indeterminate"/>
                                </vsm:VisualStateGroup>
                            </vsm:VisualStateManager.VisualStateGroups>
                            <Rectangle x:Name="FastButtonEnabled" RadiusX="5" Effect="{DynamicResource ButtonShadowEffect}" Width="Auto" Height="Auto">
                                <Rectangle.Fill>
                                    <SolidColorBrush Color="{DynamicResource OverflownButtonColor}"/>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Rectangle x:Name="FastButtonDisabled" RadiusX="5" Effect="{DynamicResource ButtonShadowEffect}">
                                <Rectangle.Fill>
                                    <SolidColorBrush Color="{DynamicResource SelectedTabColor}"/>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Path x:Name="FastSymbol" HorizontalAlignment="Center" VerticalAlignment="Center" Width="20" Data="M12.5,13.666 L25,23.666 0,23.666 z M12.5,0 L25,10 0,10 z" Height="20" Margin="0" Stretch="Fill" RenderTransformOrigin="0.5,0.5">
                                <Path.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform/>
                                        <SkewTransform/>
                                        <RotateTransform Angle="-90"/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Path.RenderTransform>
                                <Path.Fill>
                                    <SolidColorBrush Color="{DynamicResource GeneralTextColor}"/>
                                </Path.Fill>
                            </Path>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="SoundCheckBox" TargetType="{x:Type CheckBox}">
